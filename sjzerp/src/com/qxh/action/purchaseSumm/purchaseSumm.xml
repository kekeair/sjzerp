		<property name="purchaseSummService" ref="PurchaseSummService" />
	</bean>

	<!-- 为统计图表获取当月每日数量 -->
	<bean name="/purchaseSumm/getOrderNum.do" class="com.qxh.action.purchaseSumm.getOrderNumAction">
