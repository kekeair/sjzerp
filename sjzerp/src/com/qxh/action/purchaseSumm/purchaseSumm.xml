	<bean name="/purchaseSumm/getTotalPMoneyByCustomersInDay.do" class="com.qxh.action.purchaseSumm.getToTalPMoneyByCustomersInDayAction">
		<property name="purchaseSummService" ref="PurchaseSummService" />
	</bean>
