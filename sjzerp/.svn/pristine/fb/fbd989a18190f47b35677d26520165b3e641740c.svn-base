package com.qxh.exmodel;

import java.util.ArrayList;
import java.util.List;

import com.qxh.utils.NumUtil;

public class PurchaseTotalGoods {
	
	private int goodsId;
	private String goodsNm;
	private double totalNum;
	private List<Double> cList=new ArrayList<>();
	
	public double getTotalNum() {
		return totalNum;
	}
	public void setTotalNum(double totalNum) {
		double newTotalNum = NumUtil.trim0(totalNum);
		this.totalNum = newTotalNum;
	}
	public int getGoodsId() {
		return goodsId;
	}
	public void setGoodsId(int goodsId) {
		this.goodsId = goodsId;
	}
	public String getGoodsNm() {
		return goodsNm;
	}
	public void setGoodsNm(String goodsNm) {
		this.goodsNm = goodsNm;
	}
	public List<Double> getcList() {
		return cList;
	}
	public void setcList(List<Double> cList) {
		this.cList = cList;
	}
	
}
