package com.qxh.exmodel;

public class PurchaseGoods {
	
	private int goodsId;
	private String goodsNm;
	private String unitNm;
	private String spec;
	private double orderNum;
	private double stockNum;
	private double price;
	private int purchaseListDId;
	
	private int orderIndex;
	private double frontMoney;
	
	public String getSpec() {
		return spec;
	}
	public void setSpec(String spec) {
		this.spec = spec;
	}
	public double getStockNum() {
		return stockNum;
	}
	public void setStockNum(double stockNum) {
		this.stockNum = stockNum;
	}
	public double getFrontMoney() {
		return frontMoney;
	}
	public void setFrontMoney(double frontMoney) {
		this.frontMoney = frontMoney;
	}
	public int getOrderIndex() {
		return orderIndex;
	}
	public void setOrderIndex(int orderIndex) {
		this.orderIndex = orderIndex;
	}
	public String getUnitNm() {
		return unitNm;
	}
	public void setUnitNm(String unitNm) {
		this.unitNm = unitNm;
	}
	public int getGoodsId() {
		return goodsId;
	}
	public void setGoodsId(int goodsId) {
		this.goodsId = goodsId;
	}
	public String getGoodsNm() {
		return goodsNm;
	}
	public void setGoodsNm(String goodsNm) {
		this.goodsNm = goodsNm;
	}
	public double getOrderNum() {
		return orderNum;
	}
	public void setOrderNum(double orderNum) {
		this.orderNum = orderNum;
	}
	public double getPrice() {
		return price;
	}
	public void setPrice(double price) {
		this.price = price;
	}
	public int getPurchaseListDId() {
		return purchaseListDId;
	}
	public void setPurchaseListDId(int purchaseListDId) {
		this.purchaseListDId = purchaseListDId;
	}
	
}
