public class TagGoods {

	private int atNo;
	private String goodsCode;         //物料编码
	private String tagGoodsNm;        //标签商品的名字
	private double tagGoodsPrice;	  //标签商品的价格
	private int tagId;                //标签id
	private int goodsId;			  //物品id
	private int orderIndex;
	private double price; //控制当当前索引

	// ==============G/S=====================
	
	public int getAtNo() {
		return atNo;
	}

	public String getGoodsCode() {
		return goodsCode;
	}

	public void setGoodsCode(String goodsCode) {
		this.goodsCode = goodsCode;
	}

	public double getPrice() {
		return price;
	}

	public void setPrice(double price) {
		this.price = price;
	}

	public void setAtNo(int atNo) {
		this.atNo = atNo;
	}

	public String getTagGoodsNm() {
		return tagGoodsNm;
	}

	public void setTagGoodsNm(String tagGoodsNm) {
		this.tagGoodsNm = tagGoodsNm;
	}

	public double getTagGoodsPrice() {
		return tagGoodsPrice;
	}

	public void setTagGoodsPrice(double tagGoodsPrice) {
		this.tagGoodsPrice = tagGoodsPrice;
	}

	public int getTagId() {
		return tagId;
	}

	public void setTagId(int tagId) {
		this.tagId = tagId;
	}

	public int getGoodsId() {
		return goodsId;
	}

	public void setGoodsId(int goodsId) {
		this.goodsId = goodsId;
	}
	public int getOrderIndex() {
		return orderIndex;
	}
	public void setOrderIndex(int orderIndex) {
		this.orderIndex = orderIndex;
	}

}
